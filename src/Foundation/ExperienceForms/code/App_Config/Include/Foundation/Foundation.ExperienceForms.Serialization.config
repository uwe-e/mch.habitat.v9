<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
    <sitecore role:require="Standalone or ContentManagement">
        <unicorn>
            <configurations>
                <configuration name="Foundation.ExperienceForms" description="Foundation ExperienceForms" dependencies="Foundation.Serialization" extends="Helix.Foundation">
                    <predicate>
                        <include name="Template.Forms.HideableSection" database="master" path="/sitecore/system/Settings/Forms/Field Types/Structure/Hideable Section"/>
                        <include name="Forms.Structure.HideableSection" database="master" path="/sitecore/templates/System/Forms/Fields/Hideable Section"/>
                        <include name="Core.Forms.HideableSection" database="core" path="/sitecore/client/Applications/FormsBuilder/Components/Layouts/PropertyGridForm/PageSettings/Settings/Hideable Section"/>
                        <include name="Core.Forms.PageDatasource" database="core" path="/sitecore/client/Applications/FormsBuilder/Components/Layouts/Renderings/Composites/PageDatasource"/>
                    </predicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>